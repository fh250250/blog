html
body
  font-size 14px
  font-weight normal
  font-family "Source Code Pro", "PingFang SC", "Microsoft YaHei", sans-serif
  line-height 2
  color #333
  background #fff
  ::selection
    background #262a30
    color #fff

a
  color inherit
  text-decoration none

.main-wrap
  max-width 768px
  margin 0 auto
  padding 10px
  box-sizing border-box
